      "@aiinfox/ai-ourjournal/server": ["libs/ai-ourjournal/src/server.ts"],
      "@aiinfox/ai-ourservices": ["libs/ai-ourservices/src/index.ts"],
      "@aiinfox/ai-ourservices/server": ["libs/ai-ourservices/src/server.ts"],
      "@aiinfox/ai-projects": ["libs/ai-projects/src/index.ts"],
      "@aiinfox/ai-projects/server": ["libs/ai-projects/src/server.ts"],
      "@aiinfox/ai-shared": ["libs/ai-shared/src/index.ts"],
      "@aiinfox/ai-shared/server": ["libs/ai-shared/src/server.ts"],
      "@aiinfox/ai-whyaiinfox": ["libs/ai-whyaiinfox/src/index.ts"],
      "@aiinfox/ai-whyaiinfox/server": ["libs/ai-whyaiinfox/src/server.ts"]
