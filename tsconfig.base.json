{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@aiinfox/ai-aboutus": ["libs/ai-aboutus/src/index.ts"],
      "@aiinfox/ai-aboutus/server": ["libs/ai-aboutus/src/server.ts"],
      "@aiinfox/ai-corevalues": ["libs/ai-corevalues/src/index.ts"],
      "@aiinfox/ai-corevalues/server": ["libs/ai-corevalues/src/server.ts"],
      "@aiinfox/ai-foo": ["libs/ai-foo/src/index.ts"],
      "@aiinfox/ai-foo/server": ["libs/ai-foo/src/server.ts"],
      "@aiinfox/ai-hero": ["libs/ai-hero/src/index.ts"],
      "@aiinfox/ai-hero/server": ["libs/ai-hero/src/server.ts"],
      "@aiinfox/ai-missionvision": ["libs/ai-missionvision/src/index.ts"],
      "@aiinfox/ai-missionvision/server": [
        "libs/ai-missionvision/src/server.ts"
      ],
      "@aiinfox/ai-navbar": ["libs/ai-navbar/src/index.ts"],
      "@aiinfox/ai-navbar/server": ["libs/ai-navbar/src/server.ts"],
      "@aiinfox/ai-ourexpertise": ["libs/ai-ourexpertise/src/index.ts"],
      "@aiinfox/ai-ourexpertise/server": ["libs/ai-ourexpertise/src/server.ts"],
